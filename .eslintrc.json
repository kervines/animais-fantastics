{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "comma-dangle": ["error", "only-multiline"],
    "no-param-reassign": 0,
    "import/extensions": 0,
    "operator-linebreak": ["error", "after"]
  }
}
